{
  "name": "@ebay/muse-boot-default",
  "version": "0.0.3",
  "main": "index.js",
  "license": "MIT",
  "muse": {
    "type": "boot",
    "devConfig": {
      "app": "nateapp",
      "env": "staging",
      "remotePlugins": [
        "*"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=./node_modules muse-scripts-react start",
    "build": "cross-env NODE_PATH=./node_modules muse-scripts-react build",
    "test": "cross-env NODE_PATH=./node_modules muse-scripts-react test --ci --watchAll=false --passWithNoTests --coverage",
    "prestart": "muse-ebay-dev check-updates",
    "prebuild": "muse-ebay-dev check-updates",
    "test:dev": "cross-env NODE_PATH=./node_modules muse-scripts-react test --coverage",
    "deploy": "pnpm build && muse release && muse deploy musemanager"
  },
  "devDependencies": {
    "@craco/craco": "6.4.4",
    "@ebay/muse-core": "1.0.25",
    "@ebay/muse-craco-plugin": "1.0.27",
    "@ebay/muse-ebay-dev": "^1.0.1",
    "@ebay/muse-modules": "1.0.20",
    "@ebay/muse-scripts-react": "1.0.23",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "react": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "base64-inline-loader": "^2.0.1"
  }
}
