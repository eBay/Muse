{
  "name": "@ebay/muse-boot-default",
  "version": "0.0.3",
  "main": "index.js",
  "license": "MIT",
  "muse": {
    "type": "boot",
    "devConfig": {
      "app": "nateapp",
      "env": "staging",
      "remotePlugins": [
        "*"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=./node_modules muse-scripts-react start",
    "build": "cross-env NODE_PATH=./node_modules muse-scripts-react build",
    "test": "cross-env NODE_PATH=./node_modules muse-scripts-react test --ci --watchAll=false --passWithNoTests --coverage",
    "prestart": "muse-ebay check-updates",
    "prebuild": "muse-ebay check-updates",
    "test:dev": "cross-env NODE_PATH=./node_modules muse-scripts-react test --coverage",
    "deploy": "pnpm build && muse release && muse deploy nateapp"
  },
  "devDependencies": {
    "@craco/craco": "6.4.4",
    "@ebay/muse-craco-plugin": "^1.0.14",
    "@ebay/muse-ebay-dev-config": "^1.0.4",
    "@ebay/muse-modules": "^1.0.10",
    "@ebay/muse-scripts-react": "^1.0.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "react": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ebay/muse-craco-plugin": "1.0.18",
    "@ebay/muse-ebay-dev-config": "1.0.6",
    "@ebay/muse-modules": "1.0.13",
    "@ebay/muse-scripts-react": "^1.0.10",
    "base64-inline-loader": "^2.0.1"
  }
}
