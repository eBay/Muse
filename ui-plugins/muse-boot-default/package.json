{
  "name": "@ebay/muse-boot-default",
  "version": "2.0.13",
  "main": "index.js",
  "license": "MIT",
  "muse": {
    "type": "boot",
    "devConfig": {
      "app": "musemanager",
      "env": "staging",
      "remotePlugins": [
        "*"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "start": "muse-cra-patch && craco start",
    "build": "muse-cra-patch && craco build",
    "build:dev": "muse-cra-patch && cross-env NODE_ENV=development FAST_REFRESH=false craco build",
    "test": "muse-cra-patch && craco test --ci --watchAll=false --passWithNoTests --coverage",
    "test:dev": "muse-cra-patch && craco test --coverage",
    "deploy": "pnpm build && muse release && muse deploy musemanager"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@craco/craco": "7.1.0",
    "@ebay/muse-core": "1.0.36",
    "@ebay/muse-cra-patch": "^1.0.2",
    "@ebay/muse-craco-plugin": "^3.0.19",
    "@ebay/muse-modules": "1.0.25",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "react": "^18.2.0",
    "react-scripts": "^5.0.1",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ebay/muse-dev-utils": "^1.0.53",
    "base64-inline-loader": "^2.0.1"
  }
}