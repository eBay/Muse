{
  "name": "@ebay/muse-layout-antd",
  "version": "1.1.13",
  "repository": "",
  "muse": {
    "devConfig": {
      "app": "musemanager",
      "env": "staging"
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "start": "muse-cra-patch && cross-env PORT=3035 craco start",
    "build": "muse-cra-patch && craco build",
    "build:dev": "muse-cra-patch && cross-env NODE_ENV=development FAST_REFRESH=false craco build",
    "build:test": "muse-cra-patch && cross-env MUSE_TEST_BUILD=true FAST_REFRESH=false craco build",
    "test": "muse-cra-patch && cross-env craco test --ci --watchAll=false --passWithNoTests --coverage",
    "test:dev": "muse-cra-patch && craco test --coverage",
    "test:config": "muse-cra-patch && craco test --showConfig",
    "deploy": "pnpm build && pnpm build:dev && muse release && muse deploy nateapp staging"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@craco/craco": "^7.1.0",
    "@ebay/muse-core": "^1.0.35",
    "@ebay/muse-cra-patch": "^1.0.2",
    "@ebay/muse-craco-plugin": "^3.0.13",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "craco-less": "^3.0.1",
    "cross-env": "^7.0.3",
    "jest-canvas-mock": "^2.5.2",
    "react-scripts": "^5.0.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "dependencies": {
    "@ebay/muse-lib-antd": "1.2.22",
    "@ebay/muse-lib-react": "1.2.17",
    "js-plugin": "1.1.0",
    "lodash": "4.17.21",
    "semver": "7.3.7"
  }
}