{
  "name": "@ebay/muse-layout-antd",
  "version": "1.1.3",
  "repository": "",
  "muse": {
    "devConfig": {
      "app": "nateapp",
      "env": "staging",
      "remotePlugins": [
        "@ebay/muse-boot-default#boot:http://localhost:3000/boot.js"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "start": "muse-cra-patch && cross-env NODE_PATH=./node_modules PORT=3032 craco start",
    "build": "muse-cra-patch && cross-env NODE_PATH=./node_modules craco build",
    "build:dev": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_DEV_BUILD=true FAST_REFRESH=false craco build",
    "test": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_TEST_BUILD=true craco test --ci --watchAll=false --passWithNoTests --coverage",
    "test:dev": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_TEST_BUILD=true craco test --coverage",
    "test:config": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_TEST_BUILD=true craco test --showConfig",
    "prestart": "muse-ebay-dev check-updates",
    "prebuild": "muse-ebay-dev check-updates",
    "deploy": "pnpm build && pnpm build:dev && muse release && muse deploy nateapp staging"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@changesets/cli": "^2.23.2",
    "@craco/craco": "7.0.0",
    "@ebay/muse-core": "1.0.28",
    "@ebay/muse-cra-patch": "^1.0.1",
    "@ebay/muse-craco-plugin": "3.0.0",
    "@ebay/muse-ebay-dev": "1.0.3",
    "@zarconontol/enzyme-adapter-react-18": "0.7.3",
    "craco-less": "2.0.0",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "react-scripts": "^5.0.1"
  },
  "dependencies": {
    "@ebay/muse-dev-utils": "^1.0.31",
    "@ebay/muse-lib-antd": "1.1.4",
    "@ebay/muse-lib-react": "1.1.2",
    "antd": "5.1.7",
    "js-plugin": "1.1.0",
    "lodash": "4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "7.3.7"
  },
  "pnpm2": {
    "overrides": {
      "@ebay/muse-dev-utils": "link:../../workspace/packages/muse-dev-utils",
      "@ebay/muse-craco-plugin": "link:../../workspace/packages/muse-craco-plugin"
    }
  }
}
