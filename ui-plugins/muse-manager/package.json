{
  "name": "@ebay/muse-manager",
  "version": "1.2.20",
  "private": false,
  "muse": {
    "devConfig": {
      "app": "musemanager",
      "env": "staging",
      "appOverride": {
        "noSSO": false,
        "variables": {
          "museApiEndpoint": "https://musenextsvc.vip.qa.ebay.com/api/v2"
        }
      },
      "remotePlugins": [
        "*"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "files": [
    "build"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@craco/craco": "7.1.0",
    "@ebay/muse-core": "^1.0.35",
    "@ebay/muse-cra-patch": "^1.0.2",
    "@ebay/muse-craco-plugin": "^3.0.13",
    "@ebay/muse-dev-utils": "^1.0.50",
    "@ebay/muse-ebay-craco-plugin": "^1.1.1",
    "@ebay/muse-ebay-dev": "^1.0.26",
    "@ebay/muse-express-middleware": "^1.0.41",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@vitejs/plugin-react": "4.0.0",
    "autoprefixer": "^10.4.8",
    "craco-less": "^3.0.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.16",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.1.8",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "@ebay/muse-client": "^1.0.25",
    "@ebay/muse-lib-antd": "^1.2.15",
    "@ebay/muse-lib-react": "1.2.16",
    "@ebay/muse-plugin-acl": "^1.0.11",
    "@ebay/muse-vite-plugin": "^1.0.16",
    "@tanstack/react-query": "^4.28.0",
    "find-root": "^1.1.0",
    "flat": "^5.0.2",
    "lodash": "^4.17.21",
    "pretty-ms": "^7.0.1",
    "react-color": "^2.19.3",
    "react-markdown": "^6.0.3",
    "react-time-ago": "^7.2.1",
    "semver": "^7.3.7",
    "web-vitals": "^2.1.4"
  },
  "pnpm2": {
    "overrides": {
      "@ebay/muse-craco-plugin": "link:../../workspace/packages/muse-craco-plugin"
    }
  },
  "scripts": {
    "start": "muse-cra-patch && cross-env NODE_PATH=./node_modules PORT=3061 craco start",
    "prestart": "muse-ebay-dev check-updates",
    "prebuild": "muse-ebay-dev check-updates",
    "vite": "vite --host",
    "build": "muse-cra-patch && cross-env NODE_PATH=./node_modules craco build",
    "build:dev": "muse-cra-patch && cross-env NODE_PATH=./node_modules NODE_ENV=development FAST_REFRESH=false craco build",
    "build:test": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_TEST_BUILD=true FAST_REFRESH=false craco build",
    "deploy": "pnpm build && pnpm build:dev && muse release && muse deploy musemanager staging",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
