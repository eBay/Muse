{
  "name": "@ebay/muse-manager",
  "version": "1.0.0",
  "private": false,
  "muse": {
    "devConfig": {
      "app": "musemanager",
      "env": "staging",
      "remotePlugins": "*"
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@craco/craco": "6.4.4",
    "@ebay/muse-client": "1.0.17",
    "@ebay/muse-core": "1.0.25",
    "@ebay/muse-craco-plugin": "1.0.27",
    "@ebay/muse-dev-utils": "^1.0.26",
    "@ebay/muse-ebay-dev": "^1.0.1",
    "@ebay/muse-express-middleware": "1.0.25",
    "@ebay/muse-lib-antd": "1.0.11",
    "@ebay/muse-lib-react": "1.0.23",
    "@ebay/muse-plugin-acl": "1.0.5",
    "@ebay/muse-scripts-react": "1.0.23",
    "@ebay/muse-webpack-plugin": "^1.0.22",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "pretty-ms": "7.0.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-time-ago": "^7.2.1",
    "semver": "^7.3.7",
    "tailwindcss": "^3.1.8",
    "web-vitals": "^2.1.4"
  },
  "pnpm2": {
    "overrides": {
      "@ebay/muse-webpack-plugin": "link:../../workspace/packages/muse-webpack-plugin",
      "@ebay/muse-dev-utils": "link:../../workspace/packages/muse-dev-utils",
      "@ebay/muse-craco-plugin": "link:../../workspace/packages/muse-craco-plugin"
    }
  },
  "scripts": {
    "start": "cross-env PORT=3061 NODE_PATH=./node_modules muse-scripts-react start",
    "build": "cross-env NODE_PATH=./node_modules muse-scripts-react build",
    "build:dev": "cross-env NODE_PATH=./node_modules muse-scripts-react build --dev",
    "deploy": "pnpm build && pnpm build:dev && muse release && muse deploy musemanager staging",
    "prestart": "muse-ebay-dev check-updates",
    "prebuild": "muse-ebay-dev check-updates",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "craco-less": "^2.0.0",
    "postcss": "^8.4.16"
  }
}
