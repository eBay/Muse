{
  "name": "muse-runner-ui",
  "version": "1.0.0",
  "muse": {
    "devConfig": {
      "app": "muserunner",
      "env": "staging",
      "remotePlugins": "*"
    },
    "type": "normal"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@craco/craco": "7.1.0",
    "@ebay/muse-core": "^1.0.34",
    "@ebay/muse-cra-patch": "^1.0.2",
    "@ebay/muse-craco-plugin": "^3.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "chai": "^4.3.7",
    "craco-less": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-location-mock": "^1.0.9",
    "react-scripts": "^5.0.1",
    "redux-mock-store": "^1.5.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "dependencies": {
    "@ebay/muse-lib-antd": "^1.2.21",
    "@ebay/muse-lib-react": "^1.2.18",
    "@ebay/muse-vite-plugin": "^1.0.15",
    "@vitejs/plugin-react": "^4.0.4",
    "axios": "^1.5.1",
    "js-yaml": "^4.1.0",
    "rspv2": "^2.0.4",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "xterm": "^5.2.1",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-web-links": "^0.8.0"
  },
  "scripts": {
    "start": "muse-cra-patch && cross-env PORT=3030 craco start",
    "build": "muse-cra-patch && craco build",
    "build:dev": "muse-cra-patch && cross-env NODE_ENV=development FAST_REFRESH=false craco build",
    "build:test": "muse-cra-patch && cross-env MUSE_TEST_BUILD=true FAST_REFRESH=false craco build",
    "test": "muse-cra-patch && cross-env MUSE_TEST_BUILD=true craco test --ci --watchAll=false --passWithNoTests --coverage",
    "test:dev": "muse-cra-patch && cross-env MUSE_TEST_BUILD=true craco test --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
