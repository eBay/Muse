{
  "name": "@ebay/muse-dashboard",
  "version": "1.1.2",
  "muse": {
    "devConfig": {
      "app": "musemanager",
      "env": "staging",
      "remotePlugins": "*"
    },
    "type": "normal"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@ebay/muse-lib-antd": "^1.2.10",
    "@ebay/muse-lib-react": "^1.2.12",
    "@ebay/nice-form-react": "^1.0.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@craco/craco": "7.1.0",
    "@ebay/muse-core": "^1.0.33",
    "@ebay/muse-cra-patch": "^1.0.2",
    "@ebay/muse-craco-plugin": "^3.0.12",
    "@ebay/muse-ebay-craco-plugin": "^1.1.0",
    "@ebay/muse-ebay-dev": "1.0.16",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "craco-less": "^3.0.1",
    "cross-env": "^7.0.3",
    "react-grid-layout": "^1.3.4",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.1.8"
  },
  "scripts": {
    "start": "muse-cra-patch && cross-env NODE_PATH=./node_modules PORT=3033 craco start",
    "build": "muse-cra-patch && cross-env NODE_PATH=./node_modules craco build",
    "build:dev": "muse-cra-patch && cross-env NODE_PATH=./node_modules NODE_ENV=development FAST_REFRESH=false craco build",
    "build:test": "muse-cra-patch && cross-env NODE_PATH=./node_modules NODE_ENV=development MUSE_TEST_BUILD=true FAST_REFRESH=false craco build",
    "test": "muse-cra-patch && cross-env NODE_PATH=./node_modules NODE_ENV=development MUSE_TEST_BUILD=true craco test --ci --watchAll=false --passWithNoTests --coverage",
    "prestart": "muse-ebay-dev check-updates",
    "prebuild": "muse-ebay-dev check-updates",
    "test:dev": "muse-cra-patch && cross-env NODE_PATH=./node_modules NODE_ENV=development MUSE_TEST_BUILD=true craco test --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
