{
  "name": "@ebay/muse-dashboard",
  "version": "1.0.0",
  "muse": {
    "devConfig": {
      "app": "musemanager",
      "env": "staging",
      "remotePlugins": "*"
    },
    "type": "normal"
  },
  "devDependencies": {
    "@craco/craco": "6.4.4",
    "@ebay/muse-core": "^1.0.16",
    "@ebay/muse-craco-plugin": "1.0.27",
    "@ebay/muse-ebay-dev": "^1.0.1",
    "@ebay/muse-lib-antd": "^1.0.11",
    "@ebay/muse-lib-react": "1.0.23",
    "@ebay/muse-scripts-react": "^1.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.1.8"
