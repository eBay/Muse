{
  "name": "@ebay/muse-dashboard",
  "version": "1.1.0",
  "muse": {
    "devConfig": {
      "app": "musemanager",
      "env": "staging",
      "remotePlugins": "*"
    },
    "type": "normal"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "files": [
    "build"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@craco/craco": "7.0.0",
    "@ebay/muse-core": "^1.0.28",
    "@ebay/muse-craco-plugin": "3.0.0",
    "@ebay/muse-ebay-dev": "^1.0.3",
    "@ebay/muse-lib-antd": "1.1.0",
    "@ebay/muse-lib-react": "1.1.0",
    "@ebay/muse-cra-patch": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.1.8"
  },
  "scripts": {
    "start": "muse-cra-patch && cross-env NODE_PATH=./node_modules PORT=3033 craco start",
    "build": "muse-cra-patch && cross-env NODE_PATH=./node_modules craco build",
    "build:dev": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_DEV_BUILD=true craco build",
    "test": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_TEST_BUILD=true craco test --ci --watchAll=false --passWithNoTests --coverage",
    "test:dev": "muse-cra-patch && cross-env NODE_PATH=./node_modules MUSE_TEST_BUILD=true craco test --coverage"    
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
