  },
  "files": [
    "build",
    "src",
    "lib/esm",
    "lib/cjs"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@craco/craco": "^6.4.4",
    "@ebay/muse-lib-react": "workspace:^",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "antd": "^4.21.3",
    "craco-less": "^2.0.0",
    "js-plugin": "^1.1.0",
    "json-parse-better-errors": "^1.0.2",
    "@ebay/muse-craco-plugin": "workspace:^",
    "@ebay/muse-scripts-react": "workspace:^",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env PORT=3033 muse-scripts-react start",
