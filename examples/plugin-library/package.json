    "deploy": "yarn build && yarn build:dev && muse release plugin-library && muse deploy testApp1 staging plugin-library",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "test": "muse-scripts test",
    "e2e-test": "muse-scripts e2e-test",
    "prestart2": "muse-scripts check-updates",
    "prebuild2": "muse-scripts check-updates"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
