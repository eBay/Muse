{
  "name": "plugin-library",
  "version": "0.1.8",
  "repository": "",
  "private": false,
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "muse": {
    "type": "lib",
    "devConfig": {
      "app": "nate",
      "env": "staging"
    }
  },
  "publishConfig": {
