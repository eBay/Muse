{
  "name": "@ebay/muse-boot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "muse": {
    "type": "boot",
    "devConfig": {
      "app": "app1",
      "env": "staging",
      "remotePlugins": [
        "@ebay/muse-react"
      ]
    }
  },
  "scripts": {
    "start": "cross-env NODE_PATH=./node_modules muse-scripts-react start",
    "build": "cross-env NODE_PATH=./node_modules muse-scripts-react build --verbose",
    "deploy": "yarn build && muse release muse-boot && muse deploy app1 staging muse-boot"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "react": "^18.1.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "muse-craco-plugin": "workspace:^1.0.0",
    "muse-modules": "workspace:^1.0.0",
    "muse-scripts-react": "workspace:^1.0.0"
  }
}
