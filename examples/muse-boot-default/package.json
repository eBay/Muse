{
  "name": "@ebay/muse-boot-default",
  "version": "0.0.3",
  "main": "index.js",
  "license": "MIT",
  "muse": {
    "type": "boot",
    "devConfig": {
      "app": "app1",
      "env": "staging",
      "remotePlugins": [
        "@ebay/muse-lib-react"
      ]
    }
  },
  "scripts": {
    "start": "muse-scripts-react start",
    "build": "cross-env NODE_PATH=./node_modules muse-scripts-react build",
    "test": "cross-env NODE_PATH=./node_modules muse-scripts-react test --coverage",
    "test:config": "cross-env NODE_PATH=./node_modules muse-scripts-react test --showConfig",
    "test:ci": "cross-env NODE_PATH=./node_modules muse-scripts-react test --ci --watchAll=false --passWithNoTests --coverage",
    "deploy": "yarn build && muse release && muse deploy app1 "
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "react": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ebay/muse-craco-plugin": "workspace:^",
    "@ebay/muse-modules": "^1.0.0",
    "@ebay/muse-scripts-react": "^1.0.0"
  }
}
