{
  "name": "demo-controller-plugin",
  "version": "1.0.0",
  "private": true,

  "devDependencies": {
    "@craco/craco": "7.0.0",
    "@ebay/muse-core": "^1.0.28",
    "@ebay/muse-cra-patch": "^1.0.1",
    "@ebay/muse-craco-plugin": "3.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.3",
    "react-scripts": "5.0.1"
  },
  "dependencies": {
    
    "@ebay/muse-lib-antd": "1.1.2",
    "@ebay/muse-lib-react": "1.1.2",
    "js-plugin": "^1.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "muse-cra-patch && craco start",
    "build": "muse-cra-patch && craco build",
    "build:dev": "muse-cra-patch && cross-env MUSE_DEV_BUILD=true FAST_REFRESH=false craco build",
    "test": "muse-cra-patch && cross-env MUSE_TEST_BUILD=true craco test --ci --watchAll=false --passWithNoTests --coverage",
    "test:dev": "muse-cra-patch && cross-env MUSE_TEST_BUILD=true craco test --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "muse": {
    "type": "normal",
    "devConfig": {
      "app": "myapp",
      "remotePlugins": "*"
    }
  }  
}
