{
  "name": "@ebay/muse-lib-react",
  "version": "0.0.3",
  "muse": {
    "isAppEntry": true,
    "type": "lib",
    "app": "musetpl",
    "appConfig": {
      "routerType": "browser",
      "noSSO": true
    },
    "devConfig": {
      "app": "nateapp",
      "env": "staging",
      "remotePlugins": [
        "@ebay/muse-lib-antd",
        "muse-layout-antd"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-muse"
  },
  "author": "Nate Wang <pwang7@ebay.com>",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "cross-env PORT=3030 NODE_PATH=./node_modules muse-scripts-react start",
    "build": "cross-env NODE_PATH=./node_modules muse-scripts-react build",
    "build:dev": "cross-env NODE_PATH=./node_modules muse-scripts-react build --dev",
    "test": "craco test",
    "e2e-test": "muse-scripts e2e-test",
    "prestart2": "muse-scripts check-updates",
    "prebuild2": "muse-scripts check-updates",
    "deploy": "yarn build:dev && muse release @ebay/muse-lib-react && muse deploy app1 staging @ebay/muse-lib-react"
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@craco/craco": "^6.4.5",
    "@ebay/muse-craco-plugin": "^1.0.9",
    "@ebay/muse-ebay-dev-config": "^1.0.2",
    "@ebay/muse-scripts-react": "^1.0.9",
    "@ebay/nice-modal-react": "1.2.6",
    "@types/node": "^17.0.45",
    "axios": "^0.27.2",
    "craco-less": "^2.0.0",
    "history": "^5.3.0",
    "js-plugin": "1.1.0",
    "lodash": "^4.17.21",
    "path-to-regexp": "6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-use": "^17.4.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
