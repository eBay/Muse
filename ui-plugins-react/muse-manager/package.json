{
  "name": "muse-boilerplate-cra",
  "version": "0.1.0",
  "private": true,
  "muse": {
    "devConfig": {
      "app": "nateapp",
      "env": "staging"
    }
  },
  "dependencies": {
    "@craco/craco": "6.4.4",
    "@ebay/muse-craco-plugin": "^1.0.15",
    "@ebay/muse-ebay-dev-config": "^1.0.5",
